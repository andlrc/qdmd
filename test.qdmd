# Lines starting with ``#'' will be ignored
# Statements are seperated by ``;'' or a newline
# Whitespaces and empty lines are ignored

TITLE:	Case Stage Manager
LIB:	COHDEVDB

ENTITY:	CSMHDRET00; TITLE: CSM Header Entity; INDEX: HDRCASTKN; INDEX: HDRCASTKN,HDRHDRTKN
	COLUMN: HDRHDRTKN; TYPE: integer; TITLE: CSM Header Token
	COLUMN: HDRCASTKN; TYPE: integer; TITLE: CSM Header Case Token

ENTITY:	CSMSTGET00
TITLE:	CSM Stage Entity
INDEX:	STGHDRTKN
	COLUMN:	STGSTGTKN
	TYPE:	integer
	TITLE:	CSM Stage Token
	COLUMN:	STGHDRTKN
	TYPE:	integer
	TITLE:	CSM Header Token
	COLUMN:	STGTXT
	TYPE:	varchar(256)
	TITLE:	CSM Stage Title
	COLUMN:	STGSEQ
	TYPE:	integer
	TITLE:	CSM Stage Sequence
RELATION:	CSMHDRET00.HDRHDRTKN <= CSMSTGET00.STGHDRTKN
ENTITY:	CSMSTPET00
TITLE:	CSM Step Entity
INDEX:	STPSTGTKN
INDEX:	STPDEPBEF
INDEX:	STPDEPAFT
INDEX:	STPSTPTKN,STPDEPBEF
INDEX:	STPSTPTKN,STPDEPAFT
	COLUMN:	STPSTPTKN
	TYPE:	integer
	TITLE:	CSM Step Token
	COLUMN:	STPSTGTKN
	TYPE:	integer
	TITLE:	CSM Stage Token
	COLUMN:	STPTYPE
	TYPE:	varchar(256)
	TITLE:	CSM Step Type
	COLUMN:	STPTXT
	TYPE:	varchar(256)
	TITLE:	CSM Step Title
	COLUMN:	STPSUBTXT
	TYPE:	varchar(256)
	TITLE:	CSM Step Sub Title
	COLUMN:	STPSTS
	TYPE:	integer
	TITLE:	CSM Status
	VALUES: 0:Idle,20:Active,50:Completed
	COLUMN:	STPPROP
	TYPE:	varchar(16384)
	TITLE:	CSM Step Properties
	UIFORM.type:	acetextarea
	UIFORM.height:	500
	COLUMN:	STPSEQ
	TYPE:	integer
	TITLE:	CSM Step Sequence
	COLUMN:	STPACT
	TYPE:	varchar(256)
	TITLE:	CSM Step Action
	COLUMN:	STPCHG
	TYPE:	timestamp
	TITLE:	CSM Step Changed Timestamp
	COLUMN:	STPDEPBEF
	TYPE:	integer
	TITLE:	CSM Step Status depends before completed
	COLUMN:	STPDEPAFT
	TYPE:	integer
	TITLE:	CSM Step Status depends after completed
RELATION:	CSMSTPET00.STPSTGTKN => CSMSTGET00.STGSTGTKN
RELATION:	CSMSTPET00.STPSTPTKN <= CSMSTPET00.STPDEPBEF
RELATION:	CSMSTPET00.STPSTPTKN <= CSMSTPET00.STPDEPAFT
